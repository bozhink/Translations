#, c-format
msgid "%s: Wrong type for input argument #%d: String expected.\n"
msgstr "%s: Sarrera argumentuaren okerreko mota #%d: String bat espero da.\n"

#, c-format
msgid "%s: This feature is only used on Windows.\n"
msgstr "%s: Ezaugarri hau Windows sisteman baino ez da erabiltzen.\n"

#, c-format
msgid "Unsupported language '%s'.\n"
msgstr "Hizkuntza ez sostengatua '%s'.\n"

msgid "The language for menus cannot be changed on a running console.\n"
msgstr "Menuen hizkuntza ezin da aldatu martxan dagoen kontsola batean.\n"

msgid "Restart Scilab to apply to menus.\n"
msgstr "Berpiztu Scilab menuak erabiltzeko.\n"

#, c-format
msgid "%s: Wrong type for first input argument: String expected.\n"
msgstr ""
"%s: Lehenengo sarrera argumentuaren okerreko mota: String bat espero da.\n"

#, c-format
msgid "%s: Unsupported language '%s'.\n"
msgstr "%s: Hizkuntza ez sostengatua '%s'.\n"

#, c-format
msgid "Switching to default language : '%s'.\n"
msgstr "Aldatuz hizkuntz lehenetsira: '%s'.\n"
