msgid "Polynomials"
msgstr "Polynómy"

msgid "Introduction"
msgstr "Úvod"

#, c-format
msgid "%s: Wrong size for input argument #%d: A square matrix expected.\n"
msgstr ""
"%s: Nesprávny rozmer vstupného argumentu #%d: Očakávaná je štvorcová "
"matica.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: A floating point number or polynomial "
"or rational fraction array expected.\n"
msgstr ""
"%s: Nesprávny typ vstupného argumentu  #%d: Očákávané sú číslo s plávajúcou "
"čiarkou, alebo polynóm, alebo pole racionálnych zlomkov.\n"

msgid "Computing determinant: Be patient...\n"
msgstr "Vypočítavam determinant: Buďte trpezlivý...\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: A polynomial expected.\n"
msgstr "%s: Nesprávny rozmer vstupného argumentu #%d: Očakávaný polynóm.\n"

#, c-format
msgid ""
"%s: Wrong size for input argument #%d: A rational fraction expected.\n"
msgstr ""
"%s: Nesprávny rozmer vstupného argumentu #%d: Očakávaný racionálny zlomok.\n"

#, c-format
msgid ""
"%s: Wrong size for input argument #%d: A single input, single output system "
"expected.\n"
msgstr ""
"%s: Nesprávny rozmer vstupného argumentu #%d: Očakávaný systém s jednoduchým "
"vstupom, jednoduchým výstupom.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: Linear dynamical system or polynomial "
"array expected.\n"
msgstr ""
"%s: Nesprávny typ vstupného argumentu #%d: Očakávané  je pole polynómov "
"alebo lineárny dynamický systém.\n"

#, c-format
msgid "%s: Wrong type for input argument: Polynomial array expected.\n"
msgstr "%s: Nesprávny typ vstupného argumentu:Očakávané pole polynómov.\n"

#, c-format
msgid "%s: Wrong number of input argument(s): %d expected.\n"
msgstr "%s: Zlý počet vstupných argumentov: %d očakávaných.\n"

#, c-format
msgid "%s: Unexpected type for input argument #%d.\n"
msgstr "%s: Neočakávaný typ pre vstupný argument #%d.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: A row vector expected.\n"
msgstr ""
"%s: Nesprávny rozmer vstupného argumentu #%d: Očakávaný riadkový vektor.\n"

#, c-format
msgid "%s: incompatible input arguments %d and %d\n"
msgstr "%s: nekompatibilné vstupné argumenty %d a %d\n"

#, c-format
msgid "%s: Wrong value for input argument #%d: Must be in the set {%s}.\n"
msgstr "%s: Zlá hodnota vstupného argumentu #%d: Musí byť v množine {%s}.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Polynomial expected.\n"
msgstr ""

#, c-format
msgid "%s: Input argument #%d must be real.\n"
msgstr "%s: Vstupný argument #%d musí byť reálne číslo.\n"
