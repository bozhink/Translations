#, c-format
msgid "%s: No more memory.\n"
msgstr "%s: Nedostatok pamäte.\n"

msgid "Unable to render polyline, memory full.\n"
msgstr "Neschopný renderovať poluline, plná pamäť.\n"

#, c-format
msgid ""
"Warning !!!\n"
"Scilab has found a critical error (%s).\n"
"Save your data and restart Scilab.\n"
msgstr ""
"Varovanie!!!\n"
"Scilab našiel kritickú chybu (%s).\n"
"Uložte svoje dáta a reštartujte Scilab.\n"

msgid ""
"Click on an Axes object to start rotation. Right click or ESCAPE to cancel."
msgstr ""
"Kliknite na objekt Osi pre začatie rotácie. Pravý klik alebo ESCAPE pre "
"zrušenie."

msgid "Click or press ESCAPE to terminate rotation."
msgstr "Kliknite alebo stlačte ESCAPE pre zrušenie rotácie."
