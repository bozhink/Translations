#, c-format
msgid ""
"%s: Wrong type for input argument #%d or #%d: No complex input argument "
"expected.\n"
msgstr ""
"%s: Невалиден тип на входен аргумент #%d или #%d: Не се очаква комплексен "
"входен аргумент.\n"

#, c-format
msgid "%s: Wrong value for input arguments: Must be > %d.\n"
msgstr "%s: Невалидна стойност на входните аргументи: Трябва да бъде > %d.\n"

#, c-format
msgid "%s: Wrong type for first input argument.\n"
msgstr "%s: Невалиден тип за първия входен параметър.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d.\n"
msgstr "%s: Невалиден тип на входен аргумент #%d.\n"

#, c-format
msgid "%s: Only one of arg1 and arg2 may be a vector.\n"
msgstr "%s: Само един от arg1 и arg2 може да бъде вектор.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Real matrix expected.\n"
msgstr "%s: Невалиден тип на входен аргумент #%d: Очаква се реална матрица.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: Matrix with elements in (%d,%d) "
"expected.\n"
msgstr ""
"%s: Невалидна стойност на входен аргумент #%d: Очаква се матрица с елементи в "
"(%d,%d).\n"

#, c-format
msgid "%s: overflow or underflow of an extended range number\n"
msgstr "%s: overflow or underflow of an extended range number\n"

#, c-format
msgid "%s: error number %d\n"
msgstr "%s: номер на грешка %d\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Real expected.\n"
msgstr "%s: Невалиден тип на входен аргумент #%d: Очаква се реален.\n"


