#, c-format
msgid ""
"%s: Wrong type for input argument #%d or #%d: No complex input argument "
"expected.\n"
msgstr ""
"%s: Nesprávny typ pre vstupný argument #%d alebo #%d: Očakáva sa žiaden "
"komplexný vstupný argument.\n"

#, c-format
msgid "%s: Wrong value for input arguments: Must be > %d.\n"
msgstr "%s:Nesprávna hodnota vstupného argumentu: Musí byť > %d.\n"

#, c-format
msgid "%s: Wrong type for first input argument.\n"
msgstr "%s: Nesprávny typ pre prvý vstupný argument.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d.\n"
msgstr "%s: Zlý typ vstupného argumentu #%d.\n"

#, c-format
msgid "%s: Only one of arg1 and arg2 may be a vector.\n"
msgstr "%s: Len jeden z arg1 a arg2 môže byť vektor.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Real matrix expected.\n"
msgstr ""
"%s: Nesprávny typ vstupného argumentu #%d: Očakávaná je reálna matica.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: Matrix with elements in (%d,%d) "
"expected.\n"
msgstr ""
"%s: Nesprávna hodnota vstupného argumentu #%d: Očakávaná je matica s "
"elementami medzi (%d,%d).\n"

#, c-format
msgid "%s: overflow or underflow of an extended range number\n"
msgstr "%s: pretečenie alebo podtečenie rozšíreného rozsahu čísla\n"

#, c-format
msgid "%s: error number %d\n"
msgstr "%s: chyba číslo %d\n"

